Button17.Parent = ScriptsScrollingFrame
Button17.BackgroundColor3 = Color3.fromRGB(139, 77, 66)
Button17.BackgroundTransparency = 0.800
Button17.Position = UDim2.new(0.0310880542, 0, 0.601972103, 0)
